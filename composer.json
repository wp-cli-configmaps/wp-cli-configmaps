{
    "name": "wp-cli-configmaps/wp-cli-configmaps",
    "type": "wp-cli-package",
    "description": "ConfigMaps configuration management for WordPress - Tame your wp_options using WP-CLI and git",
    "homepage": "https://github.com/wp-cli-configmaps/wp-cli-configmaps",
    "license": "GPL-2.0-only",
    "keywords": [
        "wp_options",
        "options",
        "settings",
        "configuration management",
        "wp-cli",
        "wordpress"
    ],
    "authors": [
        {
            "name": "Bostjan Skufca Jese",
            "homepage": "https://github.com/bostjan",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "wp-cli/wp-cli": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "WP\\CLI\\ConfigMaps\\": "src/"
        },
        "files": [
            "wp-cli-configmaps.php"
        ]
    },
    "support": {
        "issues": "https://github.com/wp-cli-configmaps/wp-cli-configmaps/issues"
    }
}
